{
    "name": "artisanry/settings",
    "description": "Persistent Settings with multiple Drivers for Laravel 5",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-settings",
        "Laravel settings"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8",
        "faustbrian/payload": "^1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Settings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Settings\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Settings\\SettingsServiceProvider"
            ],
            "aliases": {
                "Settings": "Artisanry\\Settings\\Facades\\Settings"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
